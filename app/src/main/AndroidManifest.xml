<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.accolade.dalycs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dalycs">
        <activity android:name=".ui.plans.newplan.NewPlanActivity"></activity>
        <activity
            android:name=".ui.plans.Plans"
            android:label="@string/title_activity_plans"
            android:theme="@style/Theme.Dalycs.NoActionBar" />
        <activity android:name=".ui.UserAuthActivity" />
        <activity android:name=".ui.OnboardingActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->

    </application>

</manifest>